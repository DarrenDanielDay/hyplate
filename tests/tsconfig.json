{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "composite": true,
    "outDir": "../test-dist",
    "moduleResolution": "Node",
    "strict": true,
    "jsx": "react-jsx",
    "jsxImportSource": "../dist",
    "preserveWatchOutput": true,
    "experimentalDecorators": true,
    "useDefineForClassFields": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false
  },
  "exclude": [],
  "include": ["."]
}
